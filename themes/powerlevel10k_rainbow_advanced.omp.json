{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
		{
          "foreground": "p:lightgrey",
          "background": "transparent",
          "style": "plain",
          "template": "\u256d\u2500",
          "type": "text"
        },
        {
          "background": "p:white",
          "foreground": "p:black",
          "style": "diamond",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
          "type": "os"
        },
        {
          "background": "p:yellow",
          "foreground": "p:black",
          "powerline_symbol": "\ue0bc",
          "style": "powerline",
          "template": "\uf0ad ",
          "type": "root"
        },
        {
          "background": "p:blue",
          "foreground": "p:white",
          "powerline_symbol": "\ue0bc",
          "properties": {
            "home_icon": "~",
            "style": "full"
          },
          "style": "powerline",
          "template": " {{ if not .Writable }}\udb80\ude50{{ else if eq .Path \"~\" }}\uf4e2{{ else }}\uf07c{{ end }}  {{ path .Path .Location }} ",
          "type": "path"
        },
        {
          "background": "p:green",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:yellow{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:bloodorange{{ end }}",
            "{{ if gt .Ahead 0 }}p:skyblue{{ end }}",
            "{{ if gt .Behind 0 }}p:intensegreen{{ end }}"
          ],
          "foreground": "p:black",
          "powerline_symbol": "\ue0bc",
          "properties": {
            "branch_icon": "\uf126 ",
            "branch_max_length": 25,
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " {{ if .UpstreamURL }}{{ url .UpstreamIcon .UpstreamURL }} {{ end }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "filler": "<p:darkgrey,transparent>Â·",
      "segments": [
        {
          "background": "p:mudgreen",
          "foreground": "p:white",
          "powerline_symbol": "\ue0bc",
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": " {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} \ue718 ",
          "type": "node"
        },
        {
          "background": "p:cyan",
          "foreground": "p:black",
          "powerline_symbol": "\ue0bc",
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue627 ",
          "type": "go"
        },
        {
          "background": "p:ocean_blue",
          "foreground": "p:black",
          "powerline_symbol": "\ue0bc",
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue624 ",
          "type": "julia"
        },
        {
          "background": "p:brightyellow",
          "foreground": "p:black",
          "powerline_symbol": "\ue0bc",
          "properties": {
            "display_mode": "files",
            "fetch_virtual_env": false
          },
          "style": "powerline",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue235 ",
          "type": "python"
        },
        {
          "background": "p:red",
          "foreground": "p:white",
          "powerline_symbol": "\ue0bc",
          "properties": {
            "display_mode": "files",
            "fetch_version": true
          },
          "style": "powerline",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue791 ",
          "type": "ruby"
        },
        {
          "background": "p:yellow",
          "foreground": "p:white",
          "powerline_symbol": "\ue0bc",
          "properties": {
            "display_mode": "files",
            "fetch_version": false
          },
          "style": "powerline",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \uf0e7",
          "type": "azfunc"
        },
        {
          "background_templates": [
            "{{if contains \"default\" .Profile}}p:yelloworange{{end}}",
            "{{if contains \"jan\" .Profile}}p:hotpink{{end}}"
          ],
          "foreground": "p:white",
          "powerline_symbol": "\ue0bc",
          "properties": {
            "display_default": false
          },
          "style": "powerline",
          "template": " {{ .Profile }}{{ if .Region }}@{{ .Region }}{{ end }} \ue7ad ",
          "type": "aws"
        },
        {
          "background": "p:darkyellow",
          "foreground": "p:black",
          "powerline_symbol": "\ue0bc",
          "style": "powerline",
          "template": " {{ .FormattedMs }} \uf252 ",
          "type": "executiontime"
        },
        {
          "background": "transparent",
          "foreground": "p:green",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:red{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "style": "diamond",
          "template": " {{ if gt .Code 0 }}{{ reason .Code }} {{ .Code }} \uf467{{ else }}\uf42e{{ end }} ",
          "type": "status"
        },
        {
          "background": "p:white",
          "foreground": "transparent",
          "style": "plain",
          "template": "\ue0bc",
          "type": "text"
        },
        {
          "background": "p:white",
          "foreground": "p:black",
          "style": "diamond",
          "template": " {{ .CurrentDate | date .Format }} \uf017 ",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
		{
          "foreground": "p:lightgrey",
          "background": "transparent",
          "style": "plain",
          "template": "\u2570\u2500",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "final_space": true,
  "shell_integration": true,
  "tooltips": [
    {
      "background": "p:ocean_blue",
      "foreground": "p:black",
      "leading_diamond": "\ue0ba",
      "style": "diamond",
      "template": " {{ .Context }}{{ if .Namespace }} :: {{ .Namespace }}{{ end }} \udb84\udcfe ",
      "tips": [
        "kubectl",
        "helm",
        "h",
        "k",
        "kc"
      ],
      "type": "kubectl"
    }
  ],
  "version": 2,
  "palette": {
    "black": "#262B44",
    "blue": "#3465a4",
	"skyblue": "#89d1dc",
	"cyan": "#00acd7",
	"ocean_blue": "#387bff",
    "green": "#228800",
	"intensegreen": "#4e9a06",
	"mudgreen": "#689f63",
    "orange": "#F07623",
	"bloodorange": "#f26d50",
    "red": "#cc2222",
	"hotpink": "#f1184c",
    "white": "#E0DEF4",
	"white2": "#d3d7cf",
    "yellow": "#F3AE35",
	"yelloworange": "#FFA400",
	"brightyellow": "#FFDE57",
	"darkyellow": "#c4a000",
	"darkgrey": "#454545",
	"lightgrey": "#888888"
  }
}
